# Supabase Configuration
# To get these values, create a new project in the Supabase dashboard:
# 1. Click on Project Settings -> API
# 2. Find Project URL and copy it to VITE_SUPABASE_URL
# 3. Find Project API keys -> anon/public and copy it to VITE_SUPABASE_ANON_KEY
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Main application URL and app name
VITE_MAIN_URL=
VITE_APP_NAME=

# Google OAuth Configuration
# Get this from Google Cloud Console -> Credentials -> OAuth 2.0 Client IDs
VITE_GOOGLE_CLIENT_ID=

# Paddle Payment Configuration
# Get these values from your Paddle Dashboard
VITE_PADDLE_KEY=
VITE_PADDLE_ENVIRONMENT=         # 'sandbox' for testing, 'production' for live
VITE_PADDLE_PRODUCT_ID=
VITE_PADDLE_YEARLY_PRICE_ID=
VITE_PADDLE_MONTHLY_PRICE_ID=

# Backend API Configuration
# For local development, use localhost
# For production, use your deployed API URL
VITE_BACKEND_URL=http://localhost:3000/api
VITE_SOCKET_URL=http://localhost:3000

# External Services
VITE_MYPROFILE_URL=

VITE_SUPPORT_EMAIL=
VITE_GITHUB_URL=https://github.com/daytimedrinkingclub/shipstation
VITE_DISCORD_URL=https://discord.gg/wMNmcmq3SX

# PostHog
VITE_PUBLIC_POSTHOG_KEY=
VITE_PUBLIC_POSTHOG_HOST=
